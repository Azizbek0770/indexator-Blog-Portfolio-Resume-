# Server Configuration
PORT=5000
NODE_ENV=development

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_PUBLISHABLE_KEY=sb_publishable_example
SUPABASE_SECRET_KEY=sb_service_role_key

# JWT Secret
JWT_SECRET=change_me_in_production

# Supabase Storage Bucket
STORAGE_BUCKET=blog_media

# CORS Configuration
# Comma-separated origins allowed in production
CORS_ALLOWED_ORIGINS=https://admin.yourdomain.com,https://www.yourdomain.com
# Set to 'true' to allow all origins in development only
CORS_ALLOW_ALL=false

# WebSocket toggle (enable only if needed)
WS_ENABLED=false

# Admin Credentials (optional for initial setup)
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=strong_password_here
